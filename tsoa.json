{
  "swagger": {
    "outputDirectory": "./",
    "entryFile": "./src/index.ts",
    "host": "localhost:8080",
    "basePath": "/",
    "tags": [
      {
        "name": "Bots",
        "description": "Botim on you, Israel."
      }
